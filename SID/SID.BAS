20 REM == poke in assembly at 0xF800 ==
30 LET MB=&HF800
100 PRINT "Poking in the assembly...";
110 READ OP
120 IF OP = 999 THEN GOTO 160
130 POKE MB, OP
140 LET MB = MB + 1
150 GOTO 110
160 PRINT "...Done!"

200 REM == JP to CLEAR_SID (c3 00 f8 = jp f800) ==
210 MB = &H2293
220 POKE MB, &HC3
230 POKE MB+1, &H1E
240 POKE MB+2, &HF8
250 PRINT "Calling CLEAR-SID()...";
260 R = USR(0)
270 PRINT "Done!"

300 REM == SETUP USRLOC to call SID_IO_BAS at 0xF824 == 
310 POKE MB+1, &H00
320 LET I = 0 : REM SETTING INTERRUPTS TO 60HZ

400 REM *** RC2014 SID SOUND DEMO ***
405 PRINT "Running RC2014 SID SOUND DEMO..."
410 LET W = 17: ON INT(RND(1)*4)+1 GOTO 420,430,440,450
420 LET W = 33: GOTO 450
430 LET W = 65: GOTO 450
440 LET W = 129
450 R=24:D=15:GOSUB 1000:R=5:D=97:GOSUB 1000
451 R=6:D=200:GOSUB 1000:R=4:D=W:GOSUB 1000
460 FOR X = 0 TO 255 STEP INT((RND(1)*15)+1)
470 R = 0 : D = X : GOSUB 1000 : R = 1 : D = 255 - X : GOSUB 1000
480 FOR Y = 0 TO 33: NEXT Y, X
490 FOR X = 0 TO 200: NEXT : R = 24 : D = 0 : GOSUB 1000
500 FOR X = 0 TO 100: NEXT : GOTO 410

1000 REM PRINT "POKE S+";R;", ";D
1005 IF D > 127 THEN D = D - 256
1007 LET ARG = (D * 256) + R + I
1008 REM PRINT "ARG=";ARG
1010 RESULT = USR(ARG)
1015 REM PRINT "USR=";RESULT
1020 RETURN

